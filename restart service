REM http://www-10.lotus.com/ldd/nd85forum.nsf/DateAllFlatWeb/46ff262d5424954185257f1800272750?OpenDocument
REM https://stackoverflow.com/questions/133883/stop-and-start-a-service-via-batch-or-cmd-file

REM Stop IBM services

start C:\Lotus\Domino\nserver.exe -q
timeout 30

net stop "Lotus Domino Server (LotusDominodata)"
if %errorlevel% == 2 echo Could not stop service Lotus Domino Server (LotusDominodata).
if %errorlevel% == 0 echo Service Lotus Domino Server (LotusDominodata) stoped successfully.

net stop "Lotus Domino Diagnostics (LotusDomino)"
if %errorlevel% == 2 echo Could not stop service Lotus Domino Diagnostics.
if %errorlevel% == 0 echo Service Lotus Domino Diagnostics stoped successfully.

timeout 30

REM Start IBM services

net start "Lotus Domino Diagnostics (LotusDomino)"
if %errorlevel% == 2 echo Could not started service Lotus Domino Diagnostics.
if %errorlevel% == 0 echo Service Lotus Domino Diagnostics started successfully.

net start "Lotus Domino Server (LotusDominoData)"
if %errorlevel% == 2 echo Could not started service Lotus Domino Server (LotusDominodata).
if %errorlevel% == 0 echo Service Lotus Domino Server (LotusDominodata) started successfully.
